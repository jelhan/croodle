    "@glint/template": "1.5.2",
    "@release-it-plugins/lerna-changelog": "6.1.0",
    "@tsconfig/ember": "3.0.6",
    "@types/luxon": "3.4.2",
    "@types/qunit": "2.19.10",
    "@types/rsvp": "4.0.9",
    "@types/sjcl": "1.0.34",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "bootstrap": "4.6.2",
    "broccoli-asset-rev": "3.0.0",
    "concurrently": "8.2.2",
