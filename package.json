    "ember-cli-clipboard": "1.2.1",
    "ember-cli-content-security-policy": "2.0.3",
    "ember-cli-dependency-checker": "3.3.3",
    "ember-cli-deprecation-workflow": "2.2.0",
    "ember-cli-htmlbars": "6.3.0",
