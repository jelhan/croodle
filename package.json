    "@glint/template": "1.5.2",
    "@release-it-plugins/lerna-changelog": "7.0.0",
    "@tsconfig/ember": "3.0.9",
    "@types/eslint__js": "8.42.3",
    "@types/luxon": "3.4.2",
    "@types/qunit": "2.19.10",
    "@types/rsvp": "4.0.9",
    "@types/sjcl": "1.0.34",
    "bootstrap": "4.6.2",
